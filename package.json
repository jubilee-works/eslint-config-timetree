{
  "name": "eslint-config-timetree",
  "version": "1.8.0",
  "description": "A sharable ESLint config for TimeTree",
  "main": "index.js",
  "repository": "https://github.com/jubilee-works/eslint-config-timetree",
  "author": "Yusuke Fujiki <yusuke@fujikkys.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "build": "rimraf dist && tsc",
    "prepare": "husky install",
    "prepublish": "yarn build",
    "release": "semantic-release"
  },
  "files": [
    "dist",
    "!dist/tests",
    "index.js",
    "react.js",
    "base.js"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "peerDependencies": {
    "eslint": "^7.32.0 || ^8.0.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@tsconfig/node16-strictest": "1.0.4",
    "@types/eslint": "8.4.6",
    "@types/react": "18.0.21",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.24.0",
    "husky": "8.0.1",
    "react": "18.2.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "typescript": "4.8.3"
  }
}
